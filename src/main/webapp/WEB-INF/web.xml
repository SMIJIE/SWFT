<!--<!DOCTYPE web-app PUBLIC-->
<!--"-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"-->
<!--"http://java.sun.com/dtd/web-app_2_3.dtd" >-->

<web-app version="3.0">
    <display-name>Archetype Created Web Application</display-name>
    <welcome-file-list>
        <welcome-file>/index.jsp</welcome-file>
    </welcome-file-list>

    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>

    <jsp-config>
        <taglib>
            <taglib-uri>https://journaldev.com/jsp/tlds/mytags</taglib-uri>
            <taglib-location>/WEB-INF/dateFormatterTag.tld</taglib-location>
        </taglib>
    </jsp-config>

    <error-page>
        <exception-type>ua.training.controller.commands.exception.DataSqlException</exception-type>
        <location>/error.jsp</location>
    </error-page>
</web-app>
